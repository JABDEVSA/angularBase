CREATE TABLE POs (
  id int NOT NULL AUTO_INCREMENT,
  Clients_Name text,
  PO_Number text,
  PO_Date_recieved text,
  Requisition_Number text,
  Contract_Number TEXT NULL, 
  Contact_Person text,
  Requisitioner text,
  Tracking_Number text,
  Cost_Center text,
  Unloading_Point text,
  Services_Required text,
  Service_Planned text,
  Services_Completed text,
  Services_Left text,
  Unit_Cost text,
  Total_Cost text,
  Last_Delivery_Date text,
  Order_Active text,
  Order_Completed text,
  Order_Cancelled text,
  Reason_Cancellation text,
  Date_Cancelled text,
  Operator text,
  Date_Created text,
  Date_Modified datetime DEFAULT CURRENT_TIMESTAMP,
  PRIMARY KEY (id)
) ENGINE=InnoDB AUTO_INCREMENT=259 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci COMMENT='Hydro Clear Purchase Order Details'