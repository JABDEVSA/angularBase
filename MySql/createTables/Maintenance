CREATE TABLE Maintenance (
  id int NOT NULL AUTO_INCREMENT,
  DateFull text,
  Starting_Date text,
  Ending_Date text,
  Clients_Name text,
  PO_Number text,
  Machine_ID text,
  Location text,
  Services_Required text,
  Services_Planned text,
  Services_Complete text,
  Services_Left text,
  Maintenance_Active text,
  Maintenance_InProgress text,
  CheckSlip_Number text,
  CheckSlip_Checked text,
  Invoice_Number text,
  Invoice_Checked text,
  Maintenance_Completed text,
  Operator text,
  Date_Created text,
  Date_Modified datetime DEFAULT CURRENT_TIMESTAMP,
  PRIMARY KEY (id)
) ENGINE=InnoDB AUTO_INCREMENT=22 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci COMMENT='Hydro Clear Clendar Planning'